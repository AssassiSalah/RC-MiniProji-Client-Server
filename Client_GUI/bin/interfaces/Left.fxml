<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>

<HBox xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Left">
   <children>
      <VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefWidth="300.0" spacing="70.0">
         <children>
            <VBox alignment="TOP_CENTER" prefHeight="220.0" spacing="20.0">
               <children>
                  <Button fx:id="homeButton" mnemonicParsing="false" onAction="#onHomeButtonClick" prefHeight="60.0" prefWidth="240.0" text="Home">
                     <font>
                        <Font name="System" />
                     </font>
                  </Button>
                  <Button fx:id="downloadButton" mnemonicParsing="false" onAction="#onDownloadButtonClick" prefHeight="60.0" prefWidth="240.0" text="Download">
                     <font>
                        <Font name="System" />
                     </font>
                  </Button>
                  <Button fx:id="uploadButton" mnemonicParsing="false" onAction="#onUploadButtonClick" prefHeight="60.0" prefWidth="240.0" text="Upload">
                     <font>
                        <Font name="System" />
                     </font>
                  </Button>
               </children>
            </VBox>
            <VBox alignment="BOTTOM_CENTER" prefHeight="220.0" spacing="20.0">
               <children>
                  <Button fx:id="historyButton" mnemonicParsing="false" onAction="#onHistoryButtonClick" prefHeight="60.0" prefWidth="240.0" text="History">
                     <font>
                        <Font name="System" />
                     </font>
                  </Button>
                  <Button fx:id="downloadAdvanceButton" mnemonicParsing="false" onAction="#onDownloadAdvanceButtonClick" prefHeight="60.0" prefWidth="240.0" text="Adv. Download">
                     <font>
                        <Font name="System" />
                     </font>
                  </Button>
                  <Button fx:id="helpButton" mnemonicParsing="false" onAction="#onHelpButtonClick" prefHeight="60.0" prefWidth="240.0" text="About">
                     <font>
                        <Font name="System" />
                     </font>
                  </Button>
               </children>
            </VBox>
         </children>
         <padding>
            <Insets bottom="20.0" top="20.0" />
         </padding>
      </VBox>
      <Line endY="549.0" stroke="WHITE" />
   </children>
</HBox>
